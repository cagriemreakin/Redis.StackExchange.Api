@model List<string>
@{
    ViewData["Title"] = "StringType";
}

<a asp-action="DeleteFirstItem">Remove First Element </a>
<form method="post" asp-action="Add" enctype="application/x-www-form-urlencoded">
    <input type="text" name="name"/>
    <button type="submit">Save</button>
</form>

@foreach (var item in Model)
    {
        <h2>@item <a asp-controller="ListType" asp-action="DeleteItem" asp-route-name="@item">Delete</a></h2>
    }